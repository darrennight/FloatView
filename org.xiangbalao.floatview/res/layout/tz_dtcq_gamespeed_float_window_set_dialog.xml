<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_vertical"
    android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_bg"
    android:background="#00000000"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="300dip"
        android:layout_height="180dip"
        android:background="@drawable/tz_dtcq_gamespeed_float_window_set_dialog_boder"
        android:orientation="vertical"
        android:paddingBottom="10dip"
        android:paddingLeft="5dip"
        android:paddingRight="5dip"
        android:paddingTop="10dip" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dip"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="功能设置"
                android:textColor="#E37110"
                android:textSize="16dip" />
        </LinearLayout>

        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/tz_dtcq_gamespeed_float_window_set_dialog_inter_boder"
                android:orientation="vertical" >

                <RelativeLayout
                    style="@style/tz_dtcq_gamespeed_float_window_dialog_relative_style"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tz_dtcq_gamespeed_float_window_dialog_box_exit"
                        style="@style/tz_dtcq_gamespeed_float_window_dialog_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="远征一键退出" />

                    <com.gamedashi.dtcq.floatview.ui.button.SlideButton
                        android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_slidebutton1"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#DCDCDA" />

                <RelativeLayout
                    style="@style/tz_dtcq_gamespeed_float_window_dialog_relative_style"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_mute"
                        style="@style/tz_dtcq_gamespeed_float_window_dialog_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="游戏静音" />

                    <com.gamedashi.dtcq.floatview.ui.button.SlideButton
                        android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_slidebutton2"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#DCDCDA" />

                <RelativeLayout
                    style="@style/tz_dtcq_gamespeed_float_window_dialog_relative_style"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tz_dtcq_gamespeed_float_window_dialog_box_lineup_text"
                        style="@style/tz_dtcq_gamespeed_float_window_dialog_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="竞技场阵容推荐" />

                    <com.gamedashi.dtcq.floatview.ui.button.SlideButton
                        android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_slidebutton3"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#DCDCDA" />

                <RelativeLayout
                    style="@style/tz_dtcq_gamespeed_float_window_dialog_relative_style"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tz_dtcq_gamespeed_float_window_dialog_box_strateg"
                        style="@style/tz_dtcq_gamespeed_float_window_dialog_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="资料攻略显示" />

                    <com.gamedashi.dtcq.floatview.ui.button.SlideButton
                        android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_slidebutton4"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#DCDCDA" />

                <RelativeLayout
                    style="@style/tz_dtcq_gamespeed_float_window_dialog_relative_style"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/tz_dtcq_gamespeed_float_window_dialog_box_about_text"
                        style="@style/tz_dtcq_gamespeed_float_window_dialog_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="关于" />

                    <com.gamedashi.dtcq.floatview.ui.button.SlideButton
                        android:id="@+id/tz_dtcq_gamespeed_float_window_set_dialog_slidebutton5"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true" />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>