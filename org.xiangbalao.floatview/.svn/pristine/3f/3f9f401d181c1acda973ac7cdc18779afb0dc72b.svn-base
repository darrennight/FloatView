<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="#00000000"
    android:gravity="bottom"
    android:id="@+id/tz_dtcq_gamespeed_float_window_big_bg"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/big_window_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="#00ffffff"
        android:gravity="center_horizontal"
        android:orientation="vertical" />

    <ImageView
        android:id="@+id/tz_dtcq_gamespeed_float_window_big_logo_image"
        android:layout_width="wrap_content"
        android:layout_height="20.0dip"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="5dip"
        android:background="@drawable/tz_dtcq_float_gamespeed_bg_float_window_logo"
        android:clickable="true" />

    <RelativeLayout
        android:id="@+id/big_window_layout_content"
        android:layout_width="match_parent"
        android:layout_height="60.0dip"
        android:layout_gravity="center_horizontal"
        android:background="#00000000"
        android:gravity="center" >

        <ImageButton
            android:id="@+id/tz_dtcq_gamespeed_float_window_big_thumbtack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dip"
            android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_big_boder"
            android:src="@drawable/tz_dtcq_float_gamespeed_bg_float_window_thumbtack_up" />

        <LinearLayout
            android:id="@+id/tz_dtcq_gamespeed_float_window_big_linearLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:layout_toRightOf="@id/tz_dtcq_gamespeed_float_window_big_thumbtack"
            android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_big_boder" >

            <ImageView
                android:id="@+id/tz_dtcq_gamespeed_float_window_big_sub_image"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_boder"
                android:src="@drawable/tz_dtcq_float_gamespeed_bg_float_window_left_1" />

            <TextView
                android:id="@+id/tz_dtcq_gamespeed_float_window_big_textView"
                android:layout_width="0dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageButton2"
                android:layout_weight="1"
                android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_boder"
                android:gravity="center"
                android:text="1.0"
                android:textColor="#DF7312" />

            <ImageButton
                android:id="@+id/tz_dtcq_gamespeed_float_window_big_and_image"
                  android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView1"
                android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_boder"
                android:src="@drawable/tz_dtcq_float_gamespeed_bg_float_window_right_1" />

            <ImageButton
                android:id="@+id/tz_dtcq_gamespeed_float_window_big_reset_image"
                   android:layout_width="0dip"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageButton3"
                android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_boder"
                android:src="@drawable/tz_dtcq_float_gamespeed_bg_float_window_reset" />
        </LinearLayout>

        <Button
            android:id="@+id/tz_dtcq_gamespeed_float_window_big_set_button"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="5dip"
            android:layout_toRightOf="@id/tz_dtcq_gamespeed_float_window_big_linearLayout1"
            android:background="@drawable/tz_dtcq_gamespeed_bg_float_window_bottom_big_boder"
            android:padding="5dip"
            android:text="设置"
            android:textColor="#DF7312" />
    </RelativeLayout>

</LinearLayout>